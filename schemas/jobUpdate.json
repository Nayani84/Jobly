{
    "$schema": "http://json-schema.org/draft-07/schema#",
    "$id": "http://example.com/example.json",
    "type": "object",
    "default": {},
    "title": "Root Schema",
    "required": [
        "title"
    ],
    "properties": {
        "title": {
            "type": "string",
            "default": "",
            "title": "The title Schema",
            "examples": [
                "Developer"
            ]
        },
        "salary": {
            "type": "integer",
            "default": 0,
            "title": "The salary Schema",
            "examples": [
                50000
            ]
        },
        "equity": {
            "type": "boolean",
            "default": false,
            "title": "The equity Schema",
            "examples": [
                true
            ]
        }
    },
    "examples": [{
        "title": "Developer",
        "salary": 50000,
        "equity": true
    }]
}